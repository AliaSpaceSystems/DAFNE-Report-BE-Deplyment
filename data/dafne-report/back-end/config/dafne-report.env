## variables to be set:
# DB:
DB_USER="<db-username>"
DB_PASS="<db-password>"
DB_NAME="<db-name>"
DB_HOST="<db-docker-ip>"
DB_PORT="<db-docker-port>"
# Back-end:
API_USERNAME="<api-username>"
API_PASSWORD="<api-password>"
# Report:
ALLOWED_ORIGIN_URLS=["<dafne-back-end-ip>"]    # This is an array of ip strings, just in case different origin are needed
LOCAL_CENTRE_NAME="<local-centre-name>"    # This string is used in the final report generation to show which centre is the report referred to


## variables that usually don't need to be changed:
REPORT_BE_PORT="3000"    # docker image internal port. No need to change
GSS_LOG_FOLDER="/usr/src/app/test_logs"    # This is the GSS logs folder mounting point (inside the docker image)
AUTOMATIC_REPORT_GENERATION_SCHEDULE="0 */2 * * *"   # "0 */2 * * *" -> every 2 hours 
REPORTS_LOG_FOLDER="./logs"    # leave it as it is unless a different path is needed
LOG_LEVEL="info"